.offer-wrapper {
    display: table;
    position: relative;
    vertical-align: bottom;
    height: 100%;
}

#offer-views .sticker-wrapper {
    width: auto;
    height: 100%;
    display: table-cell;
    position: relative;
    vertical-align: bottom;
    z-index: 1;
}
#offer-views .sticker {
    width: 20px;
    height: 60px;
    display: table;
    position: relative;
    background: #fff;
    cursor: pointer;
    border-radius: 3px 0 0 3px;
    vertical-align: bottom;
    border: #c8c7cc 1px solid;
    border-right: none;
    margin-right: -1px;
}
#offer-views .sticker:hover ul>li{
    background: #0F6D36;
}

#offer-views .sticker>ul {
    list-style: none;
    padding: 0;
    margin: 0;
    display: table-cell;
    vertical-align: middle;
    position: relative
}

#offer-views .sticker>ul>li {
    padding: 0;
    margin: 5px auto;
    width: 6px;
    height: 6px;
    border-radius: 4px;
    position: relative;
    background: #06a94e;
}

#offer-views .content-wrapper {
    width: auto;
    height: 100%;
    display: table-cell;
    position: relative;
}

#offer-views .content {
    width: 0;
    display: block;
    position: relative;
    overflow: hidden;
    height: 100%;
    -webkit-transition: 0.3s;
    -moz-transition: 0.3s;
    -o-transition: 0.3s;
    transition: 0.3s;
}

#offer-views.shown .content {width: 300px;}

#offer-views .offer-header {
    display: block;
    padding: 0 10px;
    height: 34px;
    margin-bottom: -34px;
    position: relative;
}

#offer-views .offer {
    width: 300px;
    padding-left: 10px;
    padding-right: 10px;
}
#offer-views .offer-header>div,
#offer-views .offer-header-item,
#offer-views .offer-footer{
    width: 280px;
}

#offer-views .offer-header>div {
    display: block;
    padding: 0 10px;
    border-bottom: #c8c7cc 1px solid;
    position: relative;
}
#offer-views .offer-header-item {
    display: block;
