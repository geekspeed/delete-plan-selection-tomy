<div ng-controller="AlertErrorCtrl" ng-hide="hidden2" ng-click="hide()" class="arlo-fs17 arlo-cp">
    <div class="alert-body" ng-class="{'alert-settings': getType() == 'settings',
        'alert-setup': getType() == 'setup',
        'alert-cameras': getType() == 'cameras',
        'alert-body-visible': animation}">
    <span class="alert-close"></span>
        {{alertMessage}}
    </div>
</div>